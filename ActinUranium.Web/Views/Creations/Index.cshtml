@model IEnumerable<Creation>
@{
    ViewData["Title"] = Localizer["creations"];
}

<div class="table-container">
    <div class="table-cell-container container">
        <main>
            <h1>@ViewData["Title"]</h1>
            <div class="card-group flex-wrap">
                @foreach (var creation in Model)
                {
                    <partial name="_Creation" model="creation" />
                }
            </div>
        </main>
        <partial name="_Footer" />
    </div>
</div>


